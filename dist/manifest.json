{
    "manifest_version": 3,
    "name": "DegeNav",
    "description": "Chrome extension for hopping between CB NFT and OpenSea",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "degenav_16.png"
    },
    "icons": {
        "16": "degenav_16.png",
        "48": "degenav_48.png",
        "128": "degenav_128.png"
    },
    "permissions": ["tabs", "activeTab"]
}
